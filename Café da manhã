fun main() {
    val listaCafeDaManha = listOf(
        "Pão francês",
        "Manteiga",
        "Café",
        "Leite",
        "Queijo",
        "Presunto",
        "Frutas",
        "Iogurte"
    )

    println("Lista de compras para o café da manhã:")
    for (item in listaCafeDaManha) {
        println("- $item")
    }
}
